{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug join-and-log.js",
            "program": "${workspaceFolder}/examples/join-and-log.js",
            "preLaunchTask": "build",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Running Process",
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
} 